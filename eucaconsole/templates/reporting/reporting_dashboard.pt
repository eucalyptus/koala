<metal:block use-macro="main_template" i18n:domain="eucaconsole">

<head metal:fill-slot="head_css">
</head>

<div metal:fill-slot="main_content">
    <div class="row" id="contentwrap" ng-app="ReportingPage" ng-controller="ReportingPageCtrl"> 
        <metal:breadcrumbs metal:use-macro="layout.global_macros['breadcrumbs']">
            <metal:crumbs metal:fill-slot="crumbs">
                <li class="current"><a href="#" i18n:translate="">Usage Reporting</a></li>
            </metal:crumbs>
        </metal:breadcrumbs>
        <!-- Notifications -->
        <metal:block metal:use-macro="layout.global_macros['notifications']" />
        <div class="large-12 columns">
            <dl class="tabs">
                <dd><a href="" i18n:translate="">Dashboard</a></dd>
                <dd><a href="" i18n:translate="">Reports</a></dd>
                <dd><a href="" i18n:translate="">Preferences</a></dd>
            </dl>
        </div>
    </div>
</div>

<div metal:fill-slot="tail_js">
    <script src="${request.static_path('eucaconsole:static/js/pages/reporting.js')}"></script>
</div>

</metal:block>

